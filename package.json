  "license": "ISC",
  "bugs": {
    "url": "https://github.com/code-collabo/css-helper-library/issues"
  },
  "homepage": "https://github.com/code-collabo/css-helper-library#readme",
  "dependencies": {
    "less": "^4.1.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5"
  }
}
